<script>
//检测固顶事件
var IO=document.getElementById('scolls'),Y=IO,H=0,IE6;
IE6=window.ActiveXObject&&!window.XMLHttpRequest;
while(Y){H+=Y.offsetTop;Y=Y.offsetParent};
if(IE6)
    IO.style.cssText="position:absolute;top:expression(this.fix?(document"+
        ".documentElement.scrollTop-(this.javascript||"+H+")):0)";
window.onscroll=function (){
    var d=document,s=Math.max(d.documentElement.scrollTop,document.body.scrollTop);
     if(s>H&&IO.fix||s<=H&&!IO.fix)return;
    if(!IE6)IO.style.position=IO.fix?"":"fixed";        
    IO.fix=!IO.fix;
};
try{document.execCommand("BackgroundImageCache",false,true)}catch(e){};
 //]]>
		

$().ready(function(e) {
	var area='{$GetUrldecode({$GetRequest(Area)$})$}';
	var keyword='{$GetUrldecode({$GetRequest(KeyWord)$})$}';
	var cateid='{$GetUrldecode({$GetRequest(CateID)$})$}';
	var mina='{$GetUrldecode({$GetRequest(MinA)$})$}';
	var maxa='{$GetUrldecode({$GetRequest(MaxA)$})$}';
	if(area!="")
	{
		$("#area_ul li").each(function(index, element) {
			if($(this).find("a").html()==area)
				$(this).addClass("active");
		});
	}
	else
		$("#area_ul li:first-child").addClass("active");
	if(keyword!="")
	{
		$("#key_ul li").each(function(index, element) {
			if($(this).find("a").html()==keyword)
				$(this).addClass("active");
		});
	}
	else
		$("#key_ul li:first-child").addClass("active");
	if(cateid!="")
	{
		$("#cate_ul li").each(function(index, element) {
			if(cateid=="4")
				$(this).parent().find("li:nth-child(2)").addClass("active");
			else if(cateid=="5")
				$(this).parent().find("li:nth-child(3)").addClass("active");
		});
	}
	else
		$("#cate_ul li:first-child").addClass("active");
	if(mina!=""&&maxa!="")
	{
		$("#age_ul li").each(function(index, element) {
			if($(this).find("a").html()==mina+"~"+maxa)
				$(this).addClass("active");
		});
	}
	else
		$("#age_ul li:first-child").addClass("active");
    $(".item-list1").addClass("active");
});
</script>